 board = Array.new(9){Array.new(9)}
counter = 1
for x in 0..8
for y in 0..8
board[x][y] = counter
counter = counter +1
end
end
subset = Array.new(9){Array.new(9)}
subsetCounter = 0 
 for x in 1..9
 for y in 1..9
for bx in ((x/3)*3)..((x/3)*3 + 3)
    for by  in ((y/3)*3)..((y/3)*3 + 3)
       subset[bx][by] = board[x][y]
    end
end
 end
 end